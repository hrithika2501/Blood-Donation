<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
  <meta charset="utf-8">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css"
    integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
  <link rel="stylesheet" href="/css/tables.css">
  <script src="https://kit.fontawesome.com/b50b5c3588.js" crossorigin="anonymous"></script>
  <title>Dashboard</title>
</head>

<body>
  <!--Navigation-->
  <nav class="navbar navbar-expand-md navbar-light navbar-custom sticky-top">

    <a class="navbar-brand" href="/index"><img src="/images/logo.png"></a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarResponsive">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarResponsive">
      <ul class="navbar-nav mr-auto">
        <li class="nav-item ">
          <a class="nav-link paras" href="">ADMIN DASHBOARD</a>
        </li>
      </ul>
    </div>
    <div class="collapse navbar-collapse" id="navbarResponsive">
      <ul class="navbar-nav ml-auto">
        <li class="nav-item paras">
          <a class="nav-link" href="/admin">USER</a>
        </li>
        <li class="nav-item paras active">
          <a class="nav-link" href="/table">BLOOD BANKS</a>
        </li>
        <li class="nav-item paras">
          <a class="nav-link" href="/admin/camps">CAMPS</a>
        </li>
        <li class="nav-item paras">
          <a class="nav-link" href="/login/logout">LOGOUT</a>
        </li>
      </ul>
    </div>

  </nav>

  <div class="card3">
    <table class="table table-hover" class="table table-borderless">

      <tbody>
        <tr>
          <th scope="col" class="text-center">ID</th>
          <th scope="col" class="text-center">BLOOD BANK NAME</th>
          <th scope="col" class="text-center">EMAIL</th>
          <th scope="col" class="text-center">ADDRESS</th>
          <th scope="col" class="text-center">STATE</th>
          <th scope="col" class="text-center">CITY</th>
          <th scope="col" class="text-center">PINCODE</th>
          <th scope="col" class="text-center">MOBILE NUMBER</th>
          <th scope="col" class="text-center">EDIT</th>
          <th scope="col" class="text-center">DELETE</th>

        </tr>

        <% userData.forEach(function(data){ %>
          <tr>
            <td class="text-center" class="damn_border">
              <%= data.ID %>
            </td>
            <td class="text-center" class="damn_border">
              <%= data.name %>
            </td>
            <td class="text-center" class="damn_border">
              <%= data.email %>
            </td>
            <td class="text-center" class="damn_border">
              <%= data.address %>
            </td>
            <td class="text-center" class="damn_border">
              <%= data.state %>
            </td>
            <td class="text-center" class="damn_border">
              <%= data.city %>
            </td>
            <td class="text-center" class="damn_border">
              <%= data.pincode %>
            </td>
            <td class="text-center" class="damn_border">
              <%= data.contact %>
            </td>
            
            <td class="text-center" class="damn_border"> <a href="/edit/<%= data.ID %>"><i
              class="far fa-edit"></i></a> </td>
            <td class="text-center" class="damn_border"> <a href="/delete/<%= data.ID %>"><i
              class="fas fa-trash"></i></a> </td>
          </tr>
          <% }) %>
      </tbody>
    </table>
  </div>
 

  <!--Footer-->
  <footer>
    <div class="container-fluid padding foot">
      <div class="row text-center">
        <div class="col-md-4">
          <img src="/images/rsz_raktadaan_logo.jpg">
          <hr class="light">
          <p class="paras">+91-9324040843</p>
          <p class="paras">raktdan@gmail.com</p>
          <p class="paras">A-5, Paradise, Godrej Hills</p>
          <p class="paras">Kalyan,Thane,Maharashtra,India</p>
        </div>
        <div class="col-md-4">
          <hr class="light">
          <h5 class="paras">Offline Hours</h5>
          <hr class="light">
          <p class="paras">Monday: 8am-10pm</p>
          <p class="paras">Saturday: 10am-4pm</p>
          <p class="paras">Sunday: 10am-1pm</p>
        </div>
        <div class="col-md-4">
          <hr class="light">
          <h5 class="paras">Service Areas</h5>
          <hr class="light">
          <p class="paras">Mumbai, Maharshtra, 400601</p>
          <p class="paras">Jaipur, Rajasthan, 302001</p>
          <p class="paras">Gwalior, Madhya Pradesh, 474001</p>
          <p class="paras">Trivandrum, Kerela, 695008</p>
        </div>
        <div class="col-12">
          <hr class="light-100">
          <h5 class="paras">&copy; raktDaan.com</h5>
        </div>
      </div>
    </div>
  </footer>


</body>